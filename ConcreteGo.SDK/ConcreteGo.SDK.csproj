<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Title>ConcreteGo.SDK</Title>
    <Description>.net helper for using the Sysdyne Concrete Go API.</Description>
    <PackageProjectUrl>https://github.com/dev-dave-s/ConcreteGo.SDK</PackageProjectUrl>
	<PackageLicenseExpression>MIT</PackageLicenseExpression>
	<SatelliteResourceLanguages>en-US</SatelliteResourceLanguages>
	<Version>1.1.0</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Exceptionless.DateTimeExtensions" Version="3.4.3" />
    <PackageReference Include="System.Formats.Asn1" Version="8.0.2" />
    <PackageReference Include="System.Security.Cryptography.Pkcs" Version="8.0.1" />
    <PackageReference Include="System.Text.RegularExpressions" Version="4.3.1" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\ConcreteGo.SDK.Models\ConcreteGo.SDK.Models.csproj" />
    <ProjectReference Include="..\ConcreteGo.SDK.Webcrete\ConcreteGo.SDK.Webcrete.csproj" />
  </ItemGroup>

</Project>
